{
  "name": "cozy-konnector-google",
  "version": "2.0.5",
  "description": "",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/konnectors/cozy-konnector-google.git"
  },
  "keywords": [],
  "author": "Cozy Cloud",
  "license": "AGPL-3.0",
  "main": "./src/index.js",
  "eslintConfig": {
    "extends": [
      "cozy-app"
    ]
  },
  "eslintIgnore": [
    "build"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint"
    }
  },
  "scripts": {
    "start": "node ./src/index.js",
    "token": "node ./googleHelper.js",
    "dev": "cozy-konnector-dev",
    "onDeleteAccount": "cozy-konnector-dev src/onDeleteAccount.js",
    "standalone": "cozy-run-standalone",
    "pretest": "npm run clean",
    "test": "konitor testit .",
    "jest": "jest",
    "check": "konitor check .",
    "clean": "rm -rf ./data",
    "build": "webpack",
    "precommit": "yarn lint",
    "lint": "eslint --fix .",
    "deploy": "git-directory-deploy --directory build/ --branch ${DEPLOY_BRANCH:-build} --repo=${DEPLOY_REPOSITORY:-git@github.com:konnectors/cozy-konnector-google.git}",
    "cozyPublish": "cozy-app-publish --token $REGISTRY_TOKEN --build-commit $(git rev-parse ${DEPLOY_BRANCH:-build})",
    "travisDeployKey": "./bin/generate_travis_deploy_key"
  },
  "dependencies": {
    "cozy-client": "6.58.2",
    "cozy-konnector-libs": "4.24.2",
    "googleapis": "36.0.0",
    "json-loader": "0.5.7",
    "lodash": "4.17.15",
    "p-limit": "2.2.0"
  },
  "devDependencies": {
    "chalk": "2.4.2",
    "clear": "0.1.0",
    "configstore": "4.0.0",
    "copy-webpack-plugin": "5.0.4",
    "cozy-app-publish": "0.18.3",
    "cozy-jobs-cli": "1.9.17",
    "eslint": "5.15.1",
    "eslint-config-cozy-app": "1.2.2",
    "figlet": "1.2.3",
    "git-directory-deploy": "1.5.1",
    "husky": "4.0.0-beta.1",
    "jest": "23.6.0",
    "konitor": "0.10.2",
    "minimist": "1.2.0",
    "mockdate": "2.0.5",
    "svgo": "1.3.0",
    "webpack": "4.39.3",
    "webpack-cli": "3.3.7"
  }
}
