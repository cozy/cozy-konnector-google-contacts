{
  "name": "cozy-konnector-google",
  "version": "0.1.0",
  "description": "",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/konnectors/cozy-konnector-google.git"
  },
  "keywords": [],
  "author": "",
  "license": "AGPL-3.0",
  "main": "./src/index.js",
  "scripts": {
    "start": "node ./src/index.js",
    "token": "node ./googleHelper.js",
    "dev": "cozy-run-dev",
    "standalone": "cozy-run-standalone",
    "build": "webpack",
    "format": "prettier --write 'src/**/*.{js,json}'",
    "precommit": "yarn lint",
    "lint": "eslint .",
    "deploy": "git-directory-deploy --directory build/ --branch build --repo=${DEPLOY_REPOSITORY:-https://$GITHUB_TOKEN@github.com/konnectors/cozy-konnector-google.git}",
    "deploy:prod": "git-directory-deploy --directory build/ --branch latest --repo=${DEPLOY_REPOSITORY:-https://$GITHUB_TOKEN@github.com/konnectors/cozy-konnector-google.git}"
  },
  "dependencies": {
    "cozy-konnector-libs": "3.7.4",
    "googleapis": "27.0.0"
  },
  "devDependencies": {
    "chalk": "2.3.2",
    "clear": "0.1.0",
    "configstore": "3.1.2",
    "copy-webpack-plugin": "4.5.1",
    "cozy-jobs-cli": "1.0.6",
    "eslint-config-cozy-app": "0.4.0",
    "figlet": "1.2.0",
    "git-directory-deploy": "1.5.1",
    "husky": "0.14.3",
    "minimist": "1.2.0",
    "prettier": "1.11.1",
    "webpack": "4.3.0",
    "webpack-cli": "2.0.13"
  },
  "eslintConfig": {
    "extends": [
      "eslint-config-cozy-app"
    ]
  },
  "eslintIgnore": [
    "build"
  ],
  "prettier": {
    "semi": false,
    "singleQuote": true
  }
}
